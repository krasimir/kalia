{
  "name": "kalia",
  "displayName": "Kalia",
  "description": "A thoughtful VS Code color theme",
  "publisher": "krasimir",
  "version": "1.40.0",
  "keywords": [
    "theme",
    "dark",
    "kalia"
  ],
  "icon": "icon.jpg",
  "repository": {
    "type": "git",
    "url": "https://github.com/krasimir/kalia"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Themes",
    "Languages"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:javascript",
    "onLanguage:javascriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "Kalia",
        "uiTheme": "vs-dark",
        "path": "./themes/Kalia-color-theme.json"
      }
    ],
    "commands": [
      {
        "command": "Kalia.goto",
        "title": "Kalia goto"
      }
    ],
    "keybindings": [
      {
        "command": "Kalia.goto",
        "key": "ctrl+g ctrl+g",
        "mac": "cmd+g cmd+g",
        "when": "editorTextFocus"
      }
    ]
  },
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "scripts": {
    "build:client": "webpack --mode production --config webpack.config.client.js",
    "build:server": "webpack --mode production --config webpack.config.server.js",
    "release": "yarn build:client && yarn build:server && git add . && git commit -m \"New version\" && git push origin master && vsce publish minor",
    "dev": "tsc -b -w",
    "test": "node ./__tests__/index.js"
  },
  "devDependencies": {
    "@types/lodash": "4.14.149",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.42.0",
    "code-inspector": "1.6.2",
    "lodash": "4.17.19",
    "pairify": "1.0.4",
    "ts-loader": "6.2.2",
    "typescript": "3.8.3",
    "vscode-languageclient": "5.2.1",
    "vscode-languageserver": "5.2.1",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  }
}
